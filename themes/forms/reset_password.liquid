<form method="post" action="/shop/users/reset" enctype="multipart/form-data">
    <input type="hidden" name="csrfmiddlewaretoken" value="{{ csrf_token }}" />
    <input type="hidden" name="shop_name" value="{{ shop.name }}">
    <input type="hidden" name="ue" value="{{ reset_data.ue }}">
    <input type="hidden" name="token" value="{{ reset_data.token }}">
    <input type="hidden" name="form_type" value="customer_reset">
